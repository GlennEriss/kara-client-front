{
  "name": "kara",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "emulator": "firebase emulators:start --import=./firebase-data --export-on-exit=./firebase-data",
    "emulator:clean": "npm run kill-firebase && firebase emulators:start --import=./firebase-data --export-on-exit=./firebase-data",
    "kill-firebase-js": "node ./scripts/kill-firebase.js",
    "kill-firebase": "./scripts/kill-firebase.sh",
    "emulators:stop": "powershell -Command \"Get-NetTCPConnection -LocalPort 5001,5002,8080,9000,9099,9199,9090 -ErrorAction SilentlyContinue | ForEach-Object { Stop-Process -Id $_.OwningProcess -Force -ErrorAction SilentlyContinue }\"",
    "clean-exports": "rm -rf firebase-export-*",
    "dev:emulator": "npm run copy:env:dev && next dev --turbopack & npm run emulator",
    "build:preview": "npm run copy:env:preview && next build",
    "copy:env:dev": "cp .env.dev .env.local",
    "copy:env:preview": "cp .env.preview .env.local",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx || true",
    "lint:next": "next lint",
    "test:e2e": "playwright test",
    "test:e2e:all": "playwright test --reporter=list",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report",
    "test:e2e:json": "playwright test --reporter=json > tests/results/e2e-tests.json",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:coverage:json": "vitest run --coverage --reporter=json --outputFile=tests/results/unit-tests.json",
    "test:all": "pnpm test:run && pnpm test:e2e:all",
    "test:all:report": "pnpm test:coverage:json && pnpm test:e2e:json && node scripts/generate-test-report.js",
    "typecheck": "tsc --noEmit",
    "create-dev-admin": "tsx scripts/create-dev-admin-user.ts",
    "create-preprod-admin": "tsx scripts/create-preprod-admin-user.ts",
    "seed-geography": "tsx scripts/seed-geography-test-data.ts",
    "sync-vercel-env:preview": "tsx scripts/sync-vercel-env.ts preview",
    "sync-vercel-env:prod": "tsx scripts/sync-vercel-env.ts production"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@react-pdf/renderer": "^4.3.0",
    "@tabler/icons-react": "^3.34.1",
    "@tanstack/react-query": "^5.84.1",
    "@types/xlsx": "^0.0.35",
    "browser-image-compression": "^2.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "firebase": "^12.0.0",
    "firebase-admin": "^13.4.0",
    "html2canvas": "^1.4.1",
    "input-otp": "^1.4.2",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.525.0",
    "next": "15.4.8",
    "next-themes": "^0.4.6",
    "radix-ui": "^1.4.3",
    "react": "19.1.0",
    "react-country-flag": "^3.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.61.1",
    "recharts": "^2.15.4",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "world-countries": "^5.1.0",
    "xlsx": "^0.18.5",
    "zod": "^4.0.10"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.48.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^16.0.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.1.9",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jsdom": "^24.1.0",
    "playwright": "^1.48.0",
    "tailwindcss": "^4",
    "tsx": "^4.19.2",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5",
    "typescript-eslint": "^8.52.0",
    "vitest": "^2.1.0"
  }
}
